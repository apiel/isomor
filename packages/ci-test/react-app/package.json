{
  "name": "ci-test-react-app",
  "version": "0.9.1",
  "engines": {
    "node": ">=11"
  },
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "11.13.7",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "fs-extra": "^7.0.1",
    "isomor": "^0.9.1",
    "react": "^16.8.6",
    "react-async-cache": "^0.7.0",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0",
    "run-screen": "^0.5.3",
    "typescript": "3.4.4"
  },
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "yarn isomor:build && STATIC_FOLDER=./build yarn serv",
    "prod": "yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest -c jest-e2e.config.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3005",
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "jest-puppeteer": "^4.1.1",
    "nodemon": "^1.18.11",
    "puppeteer": "^1.14.0",
    "run-screen": "^0.5.2"
  }
}
