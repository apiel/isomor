{
  "name": "isomor-example",
  "version": "0.1.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/isomor"
  },
  "proxy": "http://127.0.0.1:3005",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "^11.13.0",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "fs-extra": "^7.0.1",
    "isomor": "^0.1.8",
    "isomor-react": "^0.0.3",
    "md5": "^2.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "prod": "yarn isomor:build && yarn build && STATIC_FOLDER=./build yarn serv",
    "dev": "run-p isomor:build:dev serv:dev start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/md5": "^2.1.33",
    "isomor-server": "^0.1.8",
    "isomor-transpiler": "^0.1.8",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3"
  }
}
