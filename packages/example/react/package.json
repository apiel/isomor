{
  "name": "isomor-example-react",
  "private": true,
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/isomor"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:serv": "isomor-server",
    "prod": "yarn isomor:build && yarn build && ISOMOR_STATIC_FOLDER=./build yarn isomor:serv",
    "dev": "run-screen 'yarn isomor:build --watch' 'yarn isomor:serv --watch' 'yarn start'",
    "dev:ws": "ISOMOR_WS=.* yarn dev",
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "build:stat": "yarn build --stats && npx webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "class-validator": "^0.12.2",
    "fs-extra": "^8.1.0",
    "http-proxy-middleware": "^0.20.0",
    "isomor": "^3.0.0",
    "md5": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "24.0.25",
    "@types/md5": "^2.1.33",
    "@types/node": "^13.1.2",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "isomor-server": "^3.0.0",
    "isomor-transpiler": "^3.0.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.0",
    "run-screen": "^0.5.3",
    "ts-node": "^8.5.4"
  }
}
