{
    "name": "isomor-json-schema-generator",
    "version": "2.0.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Alexandre Piel <alexandre.piel@gmail.com>",
    "license": "MIT",
    "bin": "./bin/ts-json-schema-generator",
    "repository": {
        "type": "git",
        "url": "https://github.com/apiel/isomor"
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "dependencies": {
        "@types/node": "^13.13.4",
        "commander": "~5.1.0",
        "find-up": "^4.1.0",
        "glob": "~7.1.6",
        "json-stable-stringify": "^1.0.1",
        "typescript": "~3.8.3"
    },
    "devDependencies": {
        "@types/find-up": "^4.0.0",
        "@types/glob": "^7.1.1",
        "@types/jest": "^25.2.1",
        "@types/json-stable-stringify": "^1.0.32",
        "ajv": "~6.12.2",
        "chai": "~4.2.0",
        "jest": "^25.5.4",
        "jest-junit": "^10.0.0",
        "source-map-support": "~0.5.19",
        "ts-jest": "^25.4.0",
        "ts-node": "^8.9.1",
        "tslint": "~6.1.2"
    },
    "scripts": {
        "prepublishOnly": "cp ../../Readme.sub.md ./README.md && yarn build",
        "build": "tsc && chmod +x ./bin/ts-json-schema-generator",
        "watch": "tsc -w",
        "lint": "tslint -p .",
        "fix": "tslint -p . --fix",
        "test": "jest test/ --verbose",
        "test:fast": "FAST_TEST=1 jest test/ --verbose",
        "debug": "node -r ts-node/register --inspect-brk ts-json-schema-generator.ts",
        "dev": "ts-node ts-json-schema-generator.ts"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testPathIgnorePatterns": [
            "node_modules",
            "test-runtime",
            "<rootDir>/build",
            "_site",
            "src",
            "dist"
        ],
        "coverageReporters": [
            "html",
            "cobertura"
        ],
        "coverageDirectory": "./coverage/",
        "collectCoverage": false,
        "testEnvironment": "node"
    },
    "gitHead": "2a50663b09da8ac3afba187c874fd8290cc4956b"
}
