{
  "name": "isomor-example-react",
  "private": true,
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/isomor"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "run-in-docker": "docker build . -t isomor-example-react && docker rmi isomor-example-react",
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "ISOMOR_WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "ISOMOR_STATIC_FOLDER=./build yarn serv",
    "demo": "ISOMOR_NO_VALIDATION=true yarn isomor:build && yarn build && yarn serv:prod",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start'",
    "code": "run-screen 'yarn isomor:build' 'yarn serv:dev' 'yarn start'",
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "build:stat": "yarn build --stats && npx webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "http-proxy-middleware": "^0.20.0",
    "isomor": "^2.0.0",
    "md5": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "24.0.25",
    "@types/md5": "^2.1.33",
    "@types/node": "^13.1.2",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "isomor-server": "^2.0.0",
    "isomor-transpiler": "^2.0.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.0",
    "run-screen": "^0.5.3",
    "ts-node": "^8.5.4"
  }
}
