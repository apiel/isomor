{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "STATIC_FOLDER=./dist yarn serv",
    "demo": "NO_VALIDATION=true yarn isomor:build && yarn build && yarn serv:prod",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn serve'",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "isomor": "^1.6.3",
    "vue": "^2.6.10",
    "vue-async-cache": "^0.7.0",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "isomor-server": "^1.6.3",
    "isomor-transpiler": "^1.6.3",
    "nodemon": "^2.0.1",
    "run-screen": "^0.5.3",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10",
    "yarn": "^1.19.2"
  }
}
