{
  "name": "isomor-example-react-typeorm",
  "private": true,
  "version": "1.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/isomor"
  },
  "proxy": "http://127.0.0.1:3005",
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "run-in-docker": "docker build . -t isomor-example-react && docker rmi isomor-example-react",
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "STATIC_FOLDER=./build yarn serv",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start'",
    "code": "run-screen 'yarn isomor:build' 'yarn serv:dev' 'yarn start'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:stat": "yarn build --stats && npx webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "fs-extra": "^8.0.1",
    "isomor": "^1.5.0",
    "md5": "^2.2.1",
    "react": "^16.8.6",
    "react-async-cache": "^0.7.0",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.6",
    "typeorm": "^0.2.17",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "24.0.13",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.0.2",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "isomor-server": "^1.5.0",
    "isomor-transpiler": "^1.5.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "run-screen": "^0.5.3",
    "ts-node": "^8.0.3"
  }
}
