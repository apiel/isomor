{
    "name": "isomor",
    "version": "1.8.0",
    "author": "Alexandre Piel <alexandre.piel@gmail.com>",
    "license": "MIT",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "bin": "dist/bin/index.js",
    "description": "Isomor unify the frontend and the backend by generating automatically the API",
    "homepage": "https://apiel.github.io/isomor/",
    "repository": {
        "type": "git",
        "url": "https://github.com/apiel/isomor"
    },
    "engines": {
        "node": ">=11"
    },
    "keywords": [
        "isomorphic",
        "typescript",
        "react",
        "vue",
        "angular",
        "ng",
        "nest",
        "server"
    ],
    "scripts": {
        "prepublishOnly": "cp ../../README.md . && yarn build",
        "start": "ts-node bin/index.ts",
        "prebuild": "rm -rf dist/",
        "build": "tsc -p tsconfig.json && chmod +x dist/bin/index.js",
        "lint": "tslint -p tsconfig.json -c tslint.json"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "chalk": "^3.0.0",
        "inquirer": "^7.0.0",
        "isomor-server": "^1.8.0",
        "isomor-transpiler": "^1.8.0",
        "typescript": "^3.7.2"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/chalk": "^2.2.0",
        "@types/inquirer": "^6.5.0",
        "ts-node": "^8.5.2",
        "tslint": "^5.20.1"
    },
    "gitHead": "2e27759c9ca2587c2bde419df16da27d864ab782"
