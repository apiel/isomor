{
  "name": "ci-test-react-app",
  "version": "1.12.2",
  "engines": {
    "node": ">=11"
  },
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "12.0.8",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "fs-extra": "^8.0.1",
    "isomor": "^1.12.2",
    "react": "^16.8.6",
    "react-async-cache": "^0.7.0",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "run-screen": "^0.5.3",
    "typescript": "3.5.1"
  },
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "ISOMOR_WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "ISOMOR_NO_VALIDATION=true yarn isomor:build && ISOMOR_STATIC_FOLDER=./build yarn serv",
    "prod": "yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start'",
    "code": "run-screen 'yarn isomor:build' 'yarn serv:dev' 'yarn start'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest -c jest-e2e.config.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3005",
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "jest-puppeteer": "^4.2.0",
    "nodemon": "^1.19.1",
    "puppeteer": "^1.17.0",
    "run-screen": "^0.5.2"
  }
}
