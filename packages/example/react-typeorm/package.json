{
  "name": "isomor-example-react-typeorm",
  "private": true,
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/isomor"
  },
  "proxy": "http://127.0.0.1:3005",
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:serv": "isomor-server",
    "isomor:serv:prod": "ISOMOR_STATIC_FOLDER=./build yarn isomor:serv",
    "prod": "yarn isomor:build && yarn build && yarn isomor:serv:prod",
    "dev": "run-screen 'yarn isomor:build --watch' 'yarn isomor:serv --watch' 'yarn start'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:stat": "yarn build --stats && npx webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "fs-extra": "^9.0.0",
    "isomor": "^3.0.0",
    "md5": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.2.0",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "25.2.1",
    "@types/md5": "^2.2.0",
    "@types/node": "^13.13.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "isomor-json-schema-generator": "^3.0.0",
    "isomor-server": "^3.0.0",
    "isomor-transpiler": "^3.0.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "run-screen": "^0.5.3",
    "ts-node": "^8.10.1"
  }
}
