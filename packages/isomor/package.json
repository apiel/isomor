{
    "name": "isomor",
    "version": "2.0.0",
    "author": "Alexandre Piel <alexandre.piel@gmail.com>",
    "license": "MIT",
    "bin": "dist/index.js",
    "description": "Isomor unify the frontend and the backend by generating automatically the API",
    "homepage": "https://apiel.github.io/isomor/",
    "repository": {
        "type": "git",
        "url": "https://github.com/apiel/isomor"
    },
    "engines": {
        "node": ">=11"
    },
    "keywords": [
        "isomorphic",
        "typescript",
        "react",
        "vue",
        "angular",
        "ng",
        "nest",
        "server"
    ],
    "scripts": {
        "prepublishOnly": "cp ../../README.md . && yarn build",
        "start": "ts-node bin/index.ts",
        "prebuild": "rm -rf dist/",
        "build": "tsc -p tsconfig.json && chmod +x dist/index.js",
        "lint": "tslint -p tsconfig.json -c tslint.json"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "inquirer": "^6.3.1",
        "typescript": "^3.5.1"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/inquirer": "^6.0.3",
        "ts-node": "^8.2.0",
        "tslint": "^5.17.0"
    },
