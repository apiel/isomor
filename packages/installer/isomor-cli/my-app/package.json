{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "isomor": "^1.2.1",
    "react": "^16.8.6",
    "react-async-cache": "^0.7.0",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.1"
  },
  "scripts": {
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "rimraf ./dist-server && tsc -p tsconfig.server.json && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "STATIC_FOLDER=./build yarn serv",
    "demo": "NO_VALIDATION=true yarn isomor:build && yarn build && yarn serv:prod",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start'",
    "code": "run-screen 'yarn isomor:build' 'yarn serv:dev' 'yarn start'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:stat": "yarn build --stats && npx webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3005",
  "devDependencies": {
    "isomor-server": "^1.2.0",
    "isomor-transpiler": "^1.2.1",
    "nodemon": "^1.19.1",
    "run-screen": "^0.5.3"
  }
}
