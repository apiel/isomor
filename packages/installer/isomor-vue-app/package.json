{
    "name": "isomor-vue-app",
    "version": "2.0.3",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Alexandre Piel <alexandre.piel@gmail.com>",
    "license": "MIT",
    "bin": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/apiel/isomor"
    },
    "keywords": [
        "isomorphic",
        "typescript",
        "vue",
        "server"
    ],
    "engines": {
        "node": ">=11"
    },
    "scripts": {
        "test-example": "rm -rf my-app && yarn start my-app && cd my-app && yarn dev",
        "test-in-docker": "docker build . -t test-isomor-vue-app && docker rmi test-isomor-vue-app",
        "prepublishOnly": "cp ../../../Readme.sub.md ./README.md && cp ../../example/vue/tsconfig.server.json . && cp ../../example/vue/package.json ./package-copy.json && yarn build",
        "start": "ts-node lib/index.ts",
        "prebuild": "rm -rf dist/",
        "build": "tsc -p tsconfig.json && chmod +x dist/index.js",
        "lint": "tslint -p tsconfig.json -c tslint.json"
    },
    "dependencies": {
        "chalk": "^3.0.0",
        "fs-extra": "^8.1.0",
        "isomor-core": "^2.0.1",
        "logol": "^0.0.3",
        "minimist": "^1.2.0",
        "please-upgrade-node": "^3.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.4"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/fs-extra": "^8.0.1",
        "@types/minimist": "^1.2.0",
        "@types/node": "^13.1.1",
        "@types/rimraf": "^2.0.3",
        "rimraf": "^3.0.0",
        "tslint": "^5.20.1"
    },
    "gitHead": "2a50663b09da8ac3afba187c874fd8290cc4956b"
}
