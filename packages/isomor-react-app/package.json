{
    "name": "isomor-react-app",
    "version": "0.2.2",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Alexandre Piel <alexandre.piel@gmail.com>",
    "license": "MIT",
    "bin": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/apiel/isomor"
    },
    "scripts": {
        "test-in-docker": "docker build . -t test-create-react-app && docker rmi test-create-react-app",
        "prepublishOnly": "cp ../../Readme.sub.md ./README.md && cp ../example/tsconfig.server.json . && cp ../example/package.json ./package-copy.json && yarn build",
        "start": "ts-node lib/index.ts",
        "prebuild": "rm -rf dist/",
        "build": "tsc -p tsconfig.json && chmod +x dist/index.js",
        "lint": "tslint -p tsconfig.json -c tslint.json"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "fancy-log": "^1.3.3",
        "fs-extra": "^7.0.1",
        "minimist": "^1.2.0",
        "ts-node": "^8.0.3",
        "typescript": "^3.2.4"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/fancy-log": "^1.3.1",
        "@types/fs-extra": "^5.0.5",
        "@types/minimist": "^1.2.0",
        "@types/node": "^11.12.2",
        "@types/rimraf": "^2.0.2",
        "rimraf": "^2.6.3",
        "tslint": "^5.12.1"
    }
}
